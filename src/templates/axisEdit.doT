<label>Name</label>
<input type="text" data-id="name"/>

<label>Minimum</label>
<input type="number" data-id="minimum" step="any"/>
<label>Maximum</label>
<input type="number" data-id="maximum" step="any"/>
<label>Intervall</label>
<input type="number" data-id="interval" step="any"/>

<label>Skalierung</label>
<select data-id="type">
    <option value="linear">Linear</option>
    <option value="logarithmic">Logarithmisch</option>
</select>

<label>Typ</label>
<select data-id="axisMode">
    <option value="default">Standard</option>
    <option value="x2">x2</option>
    <option value="x10">x10</option>
</select>

<button class="h25 w100" data-id="deleteAxis">Achse l√∂schen</button>